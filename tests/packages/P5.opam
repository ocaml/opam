(* API version *)
opam-version: "1"
name: "P5"
version: "1"
maintainer: "contact@ocamlpro.com"
depends: [ "P1" ]
depopts: [ "P2" ]
build: [ [ "./build.sh" ] ]
install: [[ "mkdir" "-p" "%{lib}%/p5" ]
         [ "touch" "%{lib}%/p5/p2_present" ] {P2:installed}
         [ "touch" "%{lib}%/p5/p2_absent" ] {!P2:installed} ]
remove: [ "rm" "-rf" "%{lib}%/p5" ]
