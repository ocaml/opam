    | v -> opam_current v
  in
  let write dir (name, content) =
    let name = Filename.concat dir name in
    mkdir_p (Filename.dirname name);
    let fd = open_out name in
    List.iter (fun l -> output_string fd (l^"\n")) content;
    close_out fd
