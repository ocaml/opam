set -ue

for p in `ls REPO/packages/*/*/opam`; do
  sed -i.bak "s/archive-md5/$ARCHIVE_MD5/" $p
  sed -i.bak "s/archive-sha256/$ARCHIVE_SHA256/" $p
done
