### # repo
### <add-repo.sh>
for nv in "$@" ; do
  n=${nv%.*}
  pre=REPO/packages/$n/$nv
  mkdir -p "$pre/files"
  cat << EOF > "$pre/files/$n.install"
lib: ["$nv.t"]
EOF
  echo piou >> "$nv.t"
  tar czf "$nv.tgz" "$nv.t"
  MD5=$(openssl md5 "$nv.tgz" | cut -d' ' -f2)
  XMD5=$(openssl md5 "$pre/files/$n.install" | cut -d' ' -f2)
  basedir=`echo "$BASEDIR" | sed "s/\\\\\\\\/\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\/g"`
  cat << EOF > "$pre/opam"
opam-version:"2.0"
build: [ "test" "-f" "%{name}%.%{version}%.t" ]
dev-repo: "file://$basedir/$n"
extra-files:
  [ "$n.install" "md5=$XMD5" ]
url {
  src: "$nv.tgz"
  checksum: "md5=$MD5"
}
EOF
  sh add-pin.sh "$n"
done
