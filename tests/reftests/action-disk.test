### <add-url.sh>
for nv in "$@"; do
  arch=archive.tgz
  file="REPO/packages/${nv%.*}/$nv/opam"
  cat >> "$file" << EOF
url {
  src: "$arch"
  checksum: "md5=$MD5"
}
EOF
done
