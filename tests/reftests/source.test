### <pkg:pandore.3>
opam-version: "2.0"
### <mkurl.sh>
p=pandore.3
arch=origin
file="REPO/packages/${p%.*}/$p/opam"
echo "url {" >> $file
echo "src: \"$arch.tgz\"" >> $file
MD5=$(openssl md5 $arch.tgz | cut -d' ' -f2)
echo "checksum: \"md5=$MD5\"" >> $file
echo "}" >> $file
basedir=`echo $BASEDIR | sed "s/\\\\\\\\/\\\\\\\\\\\\\\\\/g"`
echo "dev-repo: \"git+file://${basedir}/pandev\"" >> $file
